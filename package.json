{
  "name": "chewie",
  "version": "1.11.4",
  "description": "master conferences stylesheet",
  "main": "index.html",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "xmeter": "cd node_modules/xmeter/  &&  npm install  &&  npm run build  &&  cd -",
    "jade:index": "jade {index,sponsors}.jade",
    "jade:styleguide": "jade styleguide/{index,base,obj,comp-typo,comp-ui}.jade",
    "jade:all": "npm run jade:index  &&  npm run jade:styleguide",
    "lessc:chewie": "lessc styles/chewie.less styles/chewie.css",
    "lessc:styleguide": "lessc styleguide/styles/styleguide.less styleguide/styles/styleguide.css",
    "lessc:all": "npm run lessc:chewie  &&  npm run lessc:styleguide",
    "minify": "cleancss styles/chewie.css -o styles/chewie.min.css",
    "unbuild:html": "rm {index,sponsors}.html styleguide/{index,base,obj,comp-typo,comp-ui}.html",
    "unbuild:css": "rm styles/chewie.css styles/chewie.min.css styleguide/styles/styleguide.css",
    "unbuild": "npm run unbuild:html  &&  npm run unbuild:css",
    "build": "npm run jade:all  &&  npm run lessc:all  &&  npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asce-web/chewie.git"
  },
  "author": "ASCE Web Team <webteam@asce.org> (https://github.com/asce-web)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asce-web/chewie/issues"
  },
  "homepage": "https://github.com/asce-web/chewie#readme",
  "devDependencies": {
    "clean-css": "^3.4.9",
    "jade": "^1.11.0",
    "less": "^2.6.0",
    "xmeter": "3.6.0"
  },
  "dependencies": {}
}
